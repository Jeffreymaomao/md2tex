{
  "name": "md2tex",
  "version": "1.0.1",
  "description": "transform markdown to LaTeX",
  "main": "bin/md2tex.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node bin/md2tex.js",
    "tree": " tree -a -I '.git|node_modules|test|.DS_Store'",
    "build": "pkg ./bin/md2tex.js -o ./bin/dist/md2tex -c package.json -t node16-macos,node14-linux,node14-win",
    "build-mac": "pkg ./bin/md2tex.js -o ./bin/dist/md2tex -c package.json -t node16-macos-arm64"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Jeffreymaomao/md2tex.git"
  },
  "keywords": [
    "markdown",
    "latex"
  ],
  "bin": {
    "md2tex": "bin/md2tex.js"
  },
  "author": "Chang-Mao Yang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jeffreymaomao/md2tex/issues"
  },
  "homepage": "https://github.com/Jeffreymaomao/md2tex#readme",
  "dependencies": {
    "readline": "^1.3.0",
    "remark": "^15.0.1",
    "remark-parse": "^11.0.0",
    "yargs": "^17.7.2"
  }
}
